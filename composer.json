{
    "name": "ulrack/invocation-extension",
    "description": "The invocation extension for Ulrack Services.",
    "keywords": [
        "services",
        "invocation"
    ],
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ulrack/services": "^3.0"
    },
    "authors": [
        {
            "name": "Ulrack",
            "homepage": "https://www.ulrack.com/",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ulrack\\InvocationExtension\\": "src/"
        },
        "files": [
            "locator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ulrack\\InvocationExtension\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "/.gitignore",
            "/.travis.yml",
            "/phpunit.xml",
            "/phpcs.xml",
            "/PULL_REQUEST_TEMPLATE.md",
            "/CODE_OF_CONDUCT.md",
            "/CONTRIBUTING.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
